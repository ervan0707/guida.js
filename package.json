{
  "name": "guida-js",
  "version": "0.0.0",
  "description": "A modern, lightweight onboarding library with spotlight highlighting and smooth animations",
  "main": "dist/index.js",
  "module": "dist/index.esm.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist",
    "README.md"
  ],
  "scripts": {
    "dev": "npm-run-all --parallel dev:lib dev:example",
    "dev:lib": "vite build --watch",
    "dev:example": "cd example && npm run dev",
    "build": "tsc && vite build",
    "build:watch": "vite build --watch",
    "preview": "vite preview",
    "typecheck": "tsc --noEmit",
    "example": "cd example && npm run dev"
  },
  "keywords": [
    "onboarding",
    "tutorial",
    "walkthrough",
    "spotlight",
    "guided-tour",
    "user-experience",
    "typescript",
    "javascript"
  ],
  "author": "Ervan",
  "license": "MIT",
  "devDependencies": {
    "@types/node": "^20.0.0",
    "npm-run-all": "^4.1.5",
    "terser": "^5.43.1",
    "typescript": "^5.0.0",
    "vite": "^5.0.0",
    "vite-plugin-dts": "^3.0.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/ervan0707/guida-js"
  },
  "bugs": {
    "url": "https://github.com/ervan0707/guida-js/issues"
  },
  "homepage": "https://github.com/ervan0707/guida-js#readme"
}